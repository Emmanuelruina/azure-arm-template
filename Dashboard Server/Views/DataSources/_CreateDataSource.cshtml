
<!DOCTYPE html>
@{
    Layout = null;
   
}
<html class="dashboard-server">
<head>
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1.0">
    <title>
        Create data source popup
    </title>
    @Styles.Render("~/bundles/styles/create-datasource")
    @Scripts.Render("~/bundles/scripts/create-datasource")

    @Html.Partial("~/Views/Shared/_LoaderIcon.cshtml")

   <script>
        $(document).ready(function () {
            parent.$("#create-datasource-popup_wrapper").ejWaitingPopup("hide");
        });
    </script>
</head>
<body style="background-color:white">
    <div id="PopupContainer" class="col-lg-12 no-padding dialog">       
            <div class="col-xs-12 dialog-header">
                <div class="col-xs-10 no-padding">
                    <span class="su su-widget Head-icon"></span>
                    <span class="PopupTitle extendedTitle" title="[[[Create Data Source]]]">[[[Create Data Source]]]</span>
                </div>
                <div class="no-padding">
                    <a href="javascript:void(0);" title="[[[Close]]]" class="PopupClose" onclick="parent.onCreateDataSourceDialogClose()"><span class="su su-close"></span></a>
                </div>
            </div>
            <form></form>
        </div>
    </body>
</html>
