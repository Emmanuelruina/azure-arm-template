@{
    var globalAppSettings = new TenantHandler().GetTenantConfig();
    Layout = null;
    var faviconLogo = globalAppSettings.SystemSettings.FavIcon;
    var isAdminUser = HttpContext.Current.Session["IsAdmin"] != null && Convert.ToBoolean(HttpContext.Current.Session["IsAdmin"]);
}
<!DOCTYPE html>
<html class="dashboard-server">
<head>
    <meta name="viewport" content="width=device-width" />
    <title>[[[Internal Server Error]]] - @GlobalAppSettings.BrandingDisplayName</title>
    @Styles.Render("~/bundles/styles/layout")
    @Styles.Render("~/bundles/styles/error-page-new")
    @Scripts.Render("~/bundles/scripts/error-page")
    <script>
        var setPermissionUrl = "@Url.Action("SetCookieStoragePermission", "Accounts")";
    </script>

    @Html.Partial("~/Views/Shared/_LoaderIcon.cshtml")
</head>
<body class="error-full-content">
    <div class="error-page-outer">
        <div class="error-page-content">
            <div class="error-inside">
                <div class="error-image-center-1">
                    <img src="@globalAppSettings.SystemSettings.LoginLogo" onerror="if (this.src != '@ServerAppConfig.SyncfusionLoginLogoUrl') this.src = '@ServerAppConfig.SyncfusionLoginLogoUrl';" class="error-image-1">
                </div>
                <div class="error-image-center-2">
                    <img src="@GlobalAppSettings.CdnDomain/static/error-pages/v1/500.svg" class="error-image-2">
                </div>
                <div class="error-type">[[[Internal Server Error (500)]]]</div>
                <div class="error-text">
                    [[[Sorry! The server has encountered an internal error and was unable to complete your request.]]]
                </div>
                <div class="take-me-back-to-home-page">
                    <span class="error-page-cursor">
                        <a class="error-to-beck-text-color" href="@(new UrlHelper(HttpContext.Current.Request.RequestContext, RouteTable.Routes).Action("Dashboards", "Dashboards"))">[[[Go to home page]]]</a>
                    </span>
                    @if (isAdminUser && ServerAppConfig.IsSelfHosted)
                    {
                        <span class="error-text-color"> or </span><span class="error-page-cursor">
                            <a class="error-to-beck-text-color" href="https://www.@(GlobalAppSettings.BrandingDomain)/contact" target="_blank">[[[Create a support]]]</a>
                        </span>
                        <span class="error-text-color"> ticket with application logs by referring to this</span><span class="error-page-cursor">
                            <a class="error-to-beck-text-color" href="https://help.syncfusion.com/bold-bi/on-premise/faq/where-can-i-find-the-error-and-debug-log-files" target="_blank">[[[link.]]]</a>
                        </span>
                    }
                    @if (!ServerAppConfig.IsSelfHosted)
                    {
                        <span class="error-text-color"> or </span><span class="error-page-cursor">
                            <a class="error-to-beck-text-color" href="https://www.@(GlobalAppSettings.BrandingDomain)/contact" target="_blank">[[[Create a support]]]</a>
                        </span>
                    }
                </div>
            </div>
        </div>
    </div>
    @Html.Partial("../Shared/_Footer", new ViewDataDictionary { { "appsettings", globalAppSettings } })
</body>
</html>